  },
  "devDependencies": {
    "cross-spawn": "^5.1.0",
    "husky": "^0.13.4",
    "jest": "^20.0.4"
  },
  "keywords": [
    "cleanup",
    "formatter",
    "json",
    "keys",
    "object",
    "package",
    "package.json",
